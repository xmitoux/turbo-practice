{
  "name": "@repo/database",
  "version": "1.0.0",
  "private": true,
  "exports": {
    ".": "./index.ts"
  },
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
